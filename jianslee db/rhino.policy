grant {
	permission java.security.AllPermission;
	};


grant {



  permission java.io.FilePermission "$${java.home}$${/}lib$${/}rt.jar", "read";
  permission java.io.FilePermission "$${java.home}$${/}lib$${/}jaxp.properties","read";

  // Needed by default logging configuration.
  permission java.io.FilePermission "$${rhino.dir.work}$${/}log$${/}-","read,write";

  // Java "standard" properties that can be read by anyone
  permission java.util.PropertyPermission "java.version", "read";
  permission java.util.PropertyPermission "java.vendor", "read";
  permission java.util.PropertyPermission "java.vendor.url", "read";
  permission java.util.PropertyPermission "java.class.version", "read";
  permission java.util.PropertyPermission "os.name", "read";
  permission java.util.PropertyPermission "os.version", "read";
  permission java.util.PropertyPermission "os.arch", "read";
  permission java.util.PropertyPermission "file.separator", "read";
  permission java.util.PropertyPermission "path.separator", "read";
  permission java.util.PropertyPermission "line.separator", "read";

  permission java.util.PropertyPermission "java.specification.version", "read";
  permission java.util.PropertyPermission "java.specification.vendor", "read";
  permission java.util.PropertyPermission "java.specification.name", "read";

  permission java.util.PropertyPermission "java.vm.specification.version", "read";
  permission java.util.PropertyPermission "java.vm.specification.vendor", "read";
  permission java.util.PropertyPermission "java.vm.specification.name", "read";
  permission java.util.PropertyPermission "java.vm.version", "read";
  permission java.util.PropertyPermission "java.vm.vendor", "read";
  permission java.util.PropertyPermission "java.vm.name", "read";
};

// Standard extensions get all permissions by default
grant codeBase "$${java.home.url}/lib/ext/*" {
  permission java.security.AllPermission;
};

grant codeBase "$${java.home.url}/jre/lib/ext/*" {
  permission java.security.AllPermission;
};

// Allow javac and jar full access; actual access is limited by
// the caller's security context.
grant codeBase "$${java.home.url}/../lib/tools.jar" {
  permission java.security.AllPermission;
};

grant codeBase "$${rhino.dir.base.url}/lib/RhinoSDKBoot.jar" {
  permission java.security.AllPermission;
};

grant codeBase "$${rhino.dir.base.url}/lib/RhinoSDKRuntime.jar" {
  permission java.security.AllPermission;
};

grant codeBase "$${rhino.dir.base.url}/lib/rhino-logging.jar" {
  // Needed to read log settings
  permission java.util.PropertyPermission "*", "read,write";
};

grant codeBase "$${rhino.dir.base.url}/lib/logging-ext.jar" {
  // Needed to read log settings
  permission java.util.PropertyPermission "*", "read,write";
  // Needed to listen for client connections
  permission java.net.SocketPermission "*", "listen,accept,resolve";
};

grant codeBase "$${rhino.dir.base.url}/lib/log4j.jar" {
  permission java.util.PropertyPermission "log4j.*", "read";

  permission java.io.FilePermission "$${rhino.dir.work}$${/}log$${/}-", "write,read";
  permission java.net.SocketPermission "*", "connect,resolve,listen";
};

grant codeBase "$${rhino.dir.base.url}/lib/postgresql.jar" {
  permission java.net.SocketPermission "*", "connect,resolve";
};

grant codeBase "$${rhino.dir.base.url}/lib/ojdbc6.jar" {
  permission java.net.SocketPermission "*", "connect,resolve";
  permission java.util.PropertyPermission "oracle.*", "read";
  permission java.util.PropertyPermission "javax.net.ssl.*", "read";
  permission java.util.PropertyPermission "user.name", "read";
  permission javax.management.MBeanPermission "oracle.jdbc.driver.OracleDiagnosabilityMBean", "registerMBean";
};

grant codeBase "$${rhino.dir.base.url}/lib/derby.jar" {
  permission java.util.PropertyPermission "user.dir", "read";
  permission java.util.PropertyPermission "derby.*", "read";
  permission java.io.FilePermission "$${rhino.dir.work}","read,write";
  permission java.io.FilePermission "$${rhino.dir.work}$${/}-","read,write";
  permission java.io.FilePermission "$${rhino.dir.work}$${/}${MANAGEMENT_DATABASE_NAME}$${/}-","read,write,delete";
  permission java.lang.RuntimePermission "createClassLoader";
};

// Note: the special property $${{list:local.ips}} used in SocketPermissions below will
// get replaced by a line for each local IP address on this host.
// To permit remote access to Rhino services, replace $${{list:local.ips}} with
// "*" or specific IP addresses.

grant codeBase "$${rhino.dir.base.url}/lib/rem-container.jar" {
  permission java.io.FilePermission "$${rhino.dir.base}$${/}lib$${/}rem.war", "read";
  permission java.io.FilePermission "$${rhino.dir.work}", "read,write";
  permission java.io.FilePermission "$${rhino.dir.work}$${/}-", "read,write,delete";
  permission java.util.PropertyPermission "*", "read,write";
  permission java.lang.RuntimePermission "*";
  permission java.lang.reflect.ReflectPermission "*";
  permission javax.management.MBeanPermission "*", "*";
  permission java.net.SocketPermission "$${{list:local.ips}}:${REM_PORT}", "listen,resolve";
  permission java.net.SocketPermission "$${{list:local.ips}}", "accept,connect,resolve";

  // Uncomment the following line to allow remote connections
  // permission java.net.SocketPermission "*", "accept,resolve";
};

// Element Manager library extraction location
grant codeBase "$${rhino.dir.work.url}/tmp/-" {
  permission java.io.FilePermission "$${rhino.dir.base}$${/}lib$${/}-", "read";
  permission java.io.FilePermission "$${rhino.dir.work}", "read,write";
  permission java.io.FilePermission "$${rhino.dir.work}$${/}tmp$${/}-", "read,write,delete";
  permission java.util.PropertyPermission "*", "read,write";
  permission java.lang.RuntimePermission "*";
  permission java.lang.reflect.ReflectPermission "*";
  permission javax.management.MBeanPermission "*", "*";
  permission javax.management.MBeanServerPermission "*";
};

// Resource adaptors (and other SLEE component jars) are deployed at a location
// determined at runtime, somewhere under $${rhino.dir.work}/deployments/. To
// selectively grant permissions to a particular RA, edit the
// security-permissions element in the RA's resource-adaptor-jar.xml file.
//

grant codeBase "$${rhino.dir.work.url}/deployments/-" {
  // Basic permissions required by SLEE spec for SBBs
  permission java.util.PropertyPermission "*", "read";

  // allow deployed code to read their own jars
  permission java.io.FilePermission "$${rhino.dir.work}$${/}deployments$${/}-", "read";
};

// all code can register with the management framework
grant {
    //security 
    permission javax.management.MBeanTrustPermission "register";

    //server factory 
    permission javax.management.MBeanServerPermission "createMBeanServer,findMBeanServer";

    //jmx remote server delgate
    permission javax.management.MBeanPermission "javax.management.MBeanServerDelegate","addNotificationListener,registerMBean";
    permission javax.management.MBeanPermission "*","addNotificationListener";

    //fix for JMX remote NoCallStackClassLoader
    permission java.net.SocketPermission "$${{list:local.ips}}:${JMX_SERVICE_PORT}", "connect,resolve";
    permission java.net.SocketPermission "$${{list:local.ips}}:${RHINO_SSL_PORT}", "connect,resolve";

    permission javax.management.MBeanPermission "com.opencloud.rhino.management.SleeManagementMBean#getState[javax.slee.management:name=SleeManagement]", "invoke";

    permission java.util.PropertyPermission "com.sun.org.apache.xalan.*", "read";
};


// admin role -- given only to the admin user, no MBean restrictions
grant principal com.opencloud.rhino.security.principal.RhinoRole "admin" {
    permission javax.management.MBeanPermission "*","getMBeanInfo,getAttribute,getObjectInstance,queryNames,queryMBeans,setAttribute,invoke,unregisterMBean,instantiate,getDomains,getClassLoader";
};

